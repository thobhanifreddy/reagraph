{
  "name": "reagraph",
  "version": "4.15.4",
  "description": "WebGL Node-based Graph for React",
  "scripts": {
    "build": "vite build --mode library",
    "test": "vitest --passWithNoTests",
    "test:coverage": "vitest run --coverage --passWithNoTests",
    "prettier": "prettier --loglevel warn --write 'src/**/*.{ts,tsx,js,jsx}'",
    "lint": "eslint --ext js,ts,tsx",
    "lint:fix": "eslint --ext js,ts,tsx --fix src",
    "start": "storybook dev -p 9009",
    "build-storybook": "storybook build",
    "copy": "cp CNAME storybook-static/",
    "prepare": "husky install",
    "chromatic": "npx chromatic --project-token=a7098a56897c --auto-accept-changes"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "browser": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reaviz/reagraph.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "reactjs",
    "network-graph",
    "dag",
    "directed-graph",
    "graph",
    "threejs",
    "webgl"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/reaviz/reagraph/issues"
  },
  "homepage": "https://github.com/reaviz/reagraph#readme",
  "dependencies": {
    "@react-spring/three": "9.6.1",
    "@react-three/drei": "9.79.0",
    "@react-three/fiber": "8.13.5",
    "@react-three/xr": "^5.7.1",
    "camera-controls": "^2.7.0",
    "classnames": "^2.3.1",
    "d3-array": "^3.2.4",
    "d3-force-3d": "^3.0.3",
    "d3-hierarchy": "^3.1.2",
    "d3-scale": "^4.0.2",
    "ellipsize": "^0.5.1",
    "graphology": "^0.25.4",
    "graphology-layout": "^0.6.1",
    "graphology-layout-forceatlas2": "^0.10.1",
    "graphology-layout-noverlap": "^0.4.2",
    "graphology-metrics": "^2.1.0",
    "graphology-shortest-path": "^2.0.2",
    "hold-event": "^0.2.0",
    "react-use-gesture": "^9.1.3",
    "reakeys": "^1.3.1",
    "three": "^0.154.0",
    "three-stdlib": "^2.23.13",
    "zustand": "4.3.9"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.23.3",
    "@storybook/addon-docs": "^7.6.5",
    "@storybook/addon-essentials": "^7.6.5",
    "@storybook/addon-mdx-gfm": "^7.6.5",
    "@storybook/addon-storysource": "^7.6.5",
    "@storybook/addons": "^7.6.5",
    "@storybook/react": "^7.6.5",
    "@storybook/react-vite": "^7.6.5",
    "@storybook/theming": "^7.6.5",
    "@types/d3-array": "^3.2.1",
    "@types/d3-hierarchy": "^3.1.6",
    "@types/d3-scale": "^4.0.8",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/three": "^0.154.0",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-c8": "^0.33.0",
    "autoprefixer": "^10.4.14",
    "chromatic": "^10.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.13",
    "graphology-types": "^0.24.7",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.2.0",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^9.1.0",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "stats.js": "^0.17.0",
    "storybook": "^7.6.5",
    "typescript": "^4.7.4",
    "vite": "^4.4.7",
    "vite-plugin-checker": "^0.6.1",
    "vite-plugin-css-injected-by-js": "^3.2.1",
    "vite-plugin-dts": "^3.3.1",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.33.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --ext js,ts,tsx --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
